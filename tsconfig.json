
  {
    "compilerOptions": {
      "target": "es6",
      "module": "esnext",
      "strict": true,
      "jsx": "react-jsx",
      "moduleResolution": "node",
      "esModuleInterop": true,
      "skipLibCheck": true,
      "forceConsistentCasingInFileNames": true,
      "baseUrl": "./",
      "typeRoots": ["./src/@types", "./node_modules/@types"]
    },
    "include": ["src/**/*"],
    "exclude": ["node_modules", "dist"]
  }
  